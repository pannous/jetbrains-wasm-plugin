<templateSet group="WebAssembly">
  <!-- Module template -->
  <template name="module" value="(module $$NAME$&#10;  $END$&#10;)" description="WebAssembly module" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;myModule&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- Import template -->
  <template name="import" value="(import &quot;$MODULE$&quot; &quot;$NAME$&quot; (func $$$FUNCNAME$ (type $TYPE$)))" description="Import function" toReformat="true" toShortenFQNames="true">
    <variable name="MODULE" expression="" defaultValue="&quot;env&quot;" alwaysStopAt="true" />
    <variable name="NAME" expression="" defaultValue="&quot;imported&quot;" alwaysStopAt="true" />
    <variable name="FUNCNAME" expression="" defaultValue="&quot;imported&quot;" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="&quot;0&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- Function template -->
  <template name="func" value="(func $$$NAME$ (result $RESULT$)&#10;  (local $$$LOCAL$ $LOCALTYPE$)&#10;  $END$&#10;)" description="Function with result and local" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;main&quot;" alwaysStopAt="true" />
    <variable name="RESULT" expression="" defaultValue="&quot;i64&quot;" alwaysStopAt="true" />
    <variable name="LOCAL" expression="" defaultValue="&quot;result&quot;" alwaysStopAt="true" />
    <variable name="LOCALTYPE" expression="" defaultValue="&quot;i64&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- Simple function template -->
  <template name="fn" value="(func $$$NAME$&#10;  $END$&#10;)" description="Simple function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;myFunc&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- Export template -->
  <template name="export" value="(export &quot;$NAME$&quot; (func $$$FUNCNAME$))" description="Export function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;main&quot;" alwaysStopAt="true" />
    <variable name="FUNCNAME" expression="" defaultValue="&quot;main&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

