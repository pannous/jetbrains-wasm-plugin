  <!-- local template -->
  <template name="local" value="(local $$$NAME$ $TYPE$)" description="Local variable declaration" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;var&quot;" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- param template -->
  <template name="param" value="(param $$$NAME$ $TYPE$)" description="Function parameter" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;param&quot;" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- result template -->
  <template name="result" value="(result $TYPE$)" description="Function result type" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>
</templateSet>
