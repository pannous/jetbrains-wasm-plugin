  <!-- memory.load template -->
  <template name="load" value="($TYPE$.load $END$)" description="Load from memory" toReformat="true" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- memory.store template -->
  <template name="store" value="($TYPE$.store $END$)" description="Store to memory" toReformat="true" toShortenFQNames="true">
