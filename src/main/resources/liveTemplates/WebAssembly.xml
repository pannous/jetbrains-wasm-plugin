    </context>
  </template>

  <!-- memory template -->
  <template name="memory" value="(memory $SIZE$)" description="Memory declaration" toReformat="true" toShortenFQNames="true">
    <variable name="SIZE" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- table template -->
  <template name="table" value="(table $SIZE$ funcref)" description="Table declaration" toReformat="true" toShortenFQNames="true">
    <variable name="SIZE" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
    </context>
  </template>

  <!-- global template -->
  <template name="global" value="(global $$$NAME$ (mut $TYPE$) ($TYPE$.const $VALUE$))" description="Global variable declaration" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;global&quot;" alwaysStopAt="true" />
    <variable name="TYPE" expression="" defaultValue="&quot;i32&quot;" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="&quot;0&quot;" alwaysStopAt="true" />
    <context>
      <option name="WebAssembly" value="true" />
